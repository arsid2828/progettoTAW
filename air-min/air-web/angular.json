{
  "$schema": "https://json.schemastore.org/angular.json",
  "version": 1,
  "projects": {
    "air-web": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/air-web",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "tsConfig": "tsconfig.app.json"
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "air-web:build"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}